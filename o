taskkill /f /im SystemHealthMonitor.exe; $wd="$env:TEMP\SystemHealth"; $zipPath=Join-Path ([Environment]::GetFolderPath("Downloads")) "readme.zip"; $exePath=Join-Path ([Environment]::GetFolderPath("Desktop")) "SystemHealthMonitor.exe"; if(Test-Path $exePath){Remove-Item $exePath -Force; Write-Output "Deleted: $exePath"}else{Write-Output "Not found: $exePath"}; if(Test-Path $zipPath){Remove-Item $zipPath -Force; Write-Output "Deleted: $zipPath"}else{Write-Output "Not found: $zipPath"}; if(Test-Path $wd){Remove-Item $wd -Recurse -Force; Write-Output "Deleted folder: $wd"}else{Write-Output "Not found: $wd"}; [Microsoft.PowerShell.PSConsoleReadLine]::ClearHistory(); $hist="$env:APPDATA\Microsoft\Windows\PowerShell\PSReadLine\ConsoleHost_history.txt"; if(Test-Path $hist){Remove-Item $hist -Force}; exit
